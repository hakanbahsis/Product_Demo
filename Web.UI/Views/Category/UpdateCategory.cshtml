@model Entitiy.Concrete.Category
@{
    ViewData["Title"] = "UpdateCategory";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}

<h1>Kategori Güncelleme Sayfası</h1>
<form method="post">
    @Html.Label("Kategori Id")
    @Html.TextBoxFor(x=>x.CategoryId,new {@class="form-control"})
    <br />
    @Html.Label("Kategori Adı")
    @Html.TextBoxFor(x=>x.CategoryName,new {@class="form-control"})
    <br />

    <button class="btn btn-info">Kategori Güncelle</button>
</form>

